cssowl-sprite-before-float(x, y, width, height, src, xOffset=0, yOffset=0, valign=middle)
  &:before
    content: ""
    float: left
    margin-right: xOffset
    cssowl-sprite-display-inline(x, y, width, height, src, yOffset, valign)

